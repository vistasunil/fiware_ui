#!/bin/bash
#
#  curl commands to reload the data from the previous tutorial
#
#

# set -e

echo -e "Loading \033[1;34mNGSI-LD\033[0m data using the '"${TUTORIAL_DATA_MODELS_CONTEXT}"' schema."
printf "‚è≥ Loading context data "

#
# Create Streetlight Entities in various locations across Berlin
#
curl -s -o /dev/null -X POST \
  'http://'"${CONTEXT_BROKER}"'/ngsi-ld/v1/entities/' \
  -H 'Content-Type: application/ld+json' \
  -d '{
    "id": "urn:ngsi-ld:Streetlight:streetlight:mexico:4000",
    "type": "Streetlight",
    "location": {
        "type": "GeoProperty",
        "value": {
            "type": "Point",
            "coordinates": [
                -3.16448559171566,
                40.62785133667362
            ]
        }
    },
    "areaServed": {
        "type": "Property",
        "value": "City park"
    },
    "status": {
        "type": "Property",
        "value": "ok"
    },
    "refStreetlightModel": {
        "type": "Relationship",
        "object": "urn:ngsi-ld:StreetlightModel:streetlightmodel:STEEL_Tubular_10m"
    },
    "circuit": {
        "type": "Property",
        "value": "C-456-A467"
    },
    "lanternHeight": {
        "type": "Property",
        "value": 10
    },
    "locationCategory": {
        "type": "Property",
        "value": "centralIsland"
    },
    "powerState": {
        "type": "Property",
        "value": "off"
    },
    "controllingMethod": {
        "type": "Property",
        "value": "individual"
    },
    "dateLastLampChange": {
        "type": "Property",
        "value": {
            "@type": "DateTime",
            "@value": "2016-07-08T08:02:21.753Z"
        }
    },
    "@context": [
        "https://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context.jsonld"
    ]
}'

curl -s -o /dev/null -X POST \
  'http://'"${CONTEXT_BROKER}"'/ngsi-ld/v1/entities/' \
  -H 'Content-Type: application/ld+json' \
  -d '{
    "id": "urn:ngsi-ld:Streetlight:streetlight:mexico:4567",
    "type": "Streetlight",
    "location": {
        "type": "GeoProperty",
